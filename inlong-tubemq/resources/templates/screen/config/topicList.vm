<!-- main START -->
<div class="main">
    <div class="main-hd">
        <div class="crumb-wp fl">
            <span class="cur">Topic列表</span>
        </div>
    </div>

    <!-- basic-wp START -->
    <div class="basic-wp">

        <!-- toolbar-wp START -->
        <div class="toolbar-wp">
            <ul class="toolbar-list">
                <li>
                    Topic名称：
                    <input class="m" name="" value="" type="text" id="topicNameInput">
                </li>
                <li>
                    <a href="javascript:" class="btn btn-primary s" id="queryBtn">查询</a>
                </li>
            </ul>
            <ul class="addon-list fl">
                <li>
                    <a href="javascript:;" class="btn btn-primary s js_add_one">+新增</a>
                </li>
            </ul>
        </div>
        <!-- toolbar-wp END -->

        <!-- table-wp START -->
        <div class="table-wp">
            <table id="topicList">
                <colgroup span="8">
                </colgroup>
                <thead>
                <tr>
                    <th>Topic</th>
                    <th>配置Broker数</th>
                    <th>配置分区数</th>
                    <th>运行分区数</th>
                    <th>可发布</th>
                    <th>可订阅</th>
                    <th>权限受控</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody>

                </tbody>
            </table>

        </div>
        <!-- table-wp END -->
    </div>
    <!-- basic-wp END -->
</div>
<!-- main END -->

